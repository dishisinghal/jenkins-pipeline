pipeline {
  agent any 
    stages {
     stage ('Builds') {
       steps {
          echo 'Starting Building'
          echo 'Building Project'
          echo 'Building Completed'
          echo 'Hello World'
      }
    }
     stage('Tests'){
       steps {
          echo 'Starting Testing'
          echo 'Testing Project'
          echo 'Testing Completed'
      }
    }
  }
}
        
